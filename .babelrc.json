{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "not dead",
            "chrome >= 87",
            "android >= 5"
          ]
        },
        "useBuiltIns": "usage",
        "corejs": 3,
        "modules": false
      }
    ]
  ],
  "env": {
    "modern": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "chrome": "91",
              "firefox": "78",
              "safari": "14",
              "edge": "91",
              "node": "18"
            },
            "useBuiltIns": "usage",
            "corejs": 3,
            "modules": false
          }
        ]
      ]
    }
  }
}
